# ============================================
# Patient Responsibility Agent - Environment Variables
# ============================================
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# ============================================
# AdvancedMD API Configuration
# ============================================
# AdvancedMD API credentials for patient data access
AMD_USERNAME=your_amd_username
AMD_PASSWORD=your_amd_password
AMD_OFFICE_CODE=0

# ============================================
# Zapier Webhook Configuration
# ============================================
# Zapier webhook URL for service line determination
ZAPIER_WEBHOOK_URL=https://hooks.zapier.com/hooks/catch/YOUR_HOOK_ID/YOUR_HOOK_ID/

# ============================================
# PVerify API Configuration
# ============================================
# PVerify API credentials for insurance eligibility verification
PVERIFY_CLIENT_ID=your_pverify_client_id
PVERIFY_CLIENT_SECRET=your_pverify_client_secret

# ============================================
# Database Configuration
# ============================================
# PostgreSQL database connection for agent run logging
FLEMING_DB_HOST=your_db_host
FLEMING_DB_PORT=5432
FLEMING_DB_NAME=your_database_name
FLEMING_DB_USER=your_db_username
FLEMING_DB_PASSWORD=your_db_password
FLEMING_DB_SSLMODE=require

# ============================================
# Agent Configuration
# ============================================
# Agent ID for database logging (UUID format)
AGENT_ID=your_agent_uuid_here
